            <Button fx:id="trang_chu" mnemonicParsing="false" onAction="#trangChu" prefHeight="106.67" prefWidth="180.0" style="-fx-background-color: #00305b; -fx-text-fill: #f5f5f5; -fx-border-color: #79a5cd; -fx-background-radius: 0 30 30 0; -fx-border-radius: 0 30 30 0;" text="Trang Chủ">
