<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.cnpm.controllers.phat_thuong.ThongKePhatThuongController"
            prefHeight="400.0" prefWidth="780.0">
    <TableView fx:id="table" layoutX="30.0" layoutY="208.0" prefHeight="400.0" prefWidth="720.0" style="-fx-border-color: #00305b; -fx-border-radius: 12; -fx-background-color: #fefefe; -fx-background-radius: 12;">
        <columns>
            <TableColumn fx:id="idHoKhau1Col" prefWidth="180" style="-fx-border-radius: 12; -fx-background-radius: 12;" text="ID hộ khẩu" />
            <TableColumn fx:id="hoTen1Col" prefWidth="180" style="-fx-border-radius: 12; -fx-background-radius: 12;" text="Họ Tên" />
            <TableColumn fx:id="soPhanThuong1Col" prefWidth="180" style="-fx-border-radius: 12; -fx-background-radius: 12;" text="số phần thưởng" />
            <TableColumn fx:id="tongGiaTri1Col" prefWidth="180" style="-fx-border-radius: 12; -fx-background-radius: 12;" text="tổng tiền nhận" />
        </columns>
    </TableView>
    <Label fx:id="note" layoutX="300" layoutY="75">
        <font>
            <Font name="System Bold" size="25.0" />
        </font>
    </Label>
    <TextField fx:id="keywordTextField" layoutX="30.0" layoutY="64.0" prefHeight="40.0" prefWidth="127.0" promptText="Tìm kiếm">
        <cursor>
            <Cursor fx:constant="HAND" />
        </cursor>
    </TextField>
    <ChoiceBox fx:id="optionChoiceBox" layoutX="30.0" layoutY="116.0" prefHeight="40.0" prefWidth="127.0" style="-fx-background-color: #ffffff; -fx-border-radius: 30; -fx-background-radius: 30; -fx-border-color: #00305b;" />
    <ChoiceBox fx:id="dotPhatChoiceBox" layoutX="300" layoutY="116.0" prefHeight="40.0" prefWidth="127.0" style="-fx-background-color: #ffffff; -fx-border-radius: 30; -fx-background-radius: 30; -fx-border-color: #00305b;" />
</AnchorPane>


