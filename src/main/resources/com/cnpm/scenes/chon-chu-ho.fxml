<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.cnpm.controllers.ChonChuHoController">
   <children>
      <Separator layoutY="54.0" prefHeight="6.0" prefWidth="600.0" />
      <Label layoutX="27.0" layoutY="14.0" text="Chọn chủ hộ">
         <font>
            <Font name="System Bold" size="19.0" />
         </font>
      </Label>
      <TableView fx:id="chuHoTable" layoutX="14.0" layoutY="81.0" prefHeight="300.0" prefWidth="390.0">
        <columns>
          <TableColumn fx:id="MaChuHoCol" prefWidth="130.0" text="ID" />
          <TableColumn fx:id="hoTenCol" prefWidth="130.0" text="Họ tên" />
            <TableColumn fx:id="selectCol" prefWidth="130.0" text="Tuổi" />
        </columns>
      </TableView>
      <Button fx:id="submitBtn" layoutX="515.0" layoutY="347.0" mnemonicParsing="false" onAction="#submit" text="Xác nhận" />
      <TextField fx:id="hoTenChuHoTxt" editable="false" layoutX="416.0" layoutY="107.0" />
      <Button fx:id="cancleBtn" layoutX="429.0" layoutY="347.0" mnemonicParsing="false" onAction="#cancle" text="Hủy" />
      <Label layoutX="415.0" layoutY="81.0" text="Họ tên chủ hộ" />
      <Label fx:id="errorLabel" layoutX="416.0" layoutY="145.0" text="Label" />
   </children>
</AnchorPane>
